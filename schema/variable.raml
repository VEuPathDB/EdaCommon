#%RAML 1.0 Library

types:
  # Type name determines the name of the Java class that will be generated.

  VariableSpec:
    additionalProperties: false
    properties:
      entityId: string
      variableId: string

  DerivationType:
    type: string
    enum: ['transform', 'reduction']

  DerivedVariable:
    type: VariableSpec
    properties:
      variableType: API_VariableType
      derivationType: DerivationType
      dependentVariables: VariableSpec[]
      construction: object # TBD
